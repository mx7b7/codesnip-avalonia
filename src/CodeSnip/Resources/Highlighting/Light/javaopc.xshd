<?xml version="1.0" encoding="UTF-8" ?>
<SyntaxDefinition name="JavaOpcodes" extensions=".javaopc" xmlns="http://icsharpcode.net/sharpdevelop/syntaxdefinition/2008">
	<Color name="Keyword" foreground="Blue" />
	<Color name="Instruction" foreground="Brown" />
	<Color name="Number" foreground="DarkBlue" />
	<Color name="Comment" foreground="SlateGray" />
	<Color name="Label" foreground="Teal" />
	<!-- <Color name="DisassemblyKeyword" foreground="#795E26" /> -->

	<Property name="Extension" value="javaopc" />

	<RuleSet>
		<Keywords color="Keyword">
			<Word>public</Word>
			<Word>private</Word>
			<Word>protected</Word>
			<Word>static</Word>
