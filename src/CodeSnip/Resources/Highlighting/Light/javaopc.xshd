		<Rule color="Comment">//.*$</Rule>

		<!-- Labels: disassembly addresses (e.g., 0:, 1:) -->
		<Rule color="Label">^\s*\d+:</Rule>

		<Rule color="Number">\b\d+\b</Rule>

